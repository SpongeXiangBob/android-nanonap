        <!-- Customize your theme here -->
        <item name="colorPrimary">@color/colorPrimaryDarkMode</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkDarkMode</item>
        <item name="colorAccent">@color/colorAccentDarkMode</item>
